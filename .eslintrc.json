{
    "extends": [
    "react-app",
    "react-app/jest"
],
    "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
        "no-unused-vars": "warn",
        "@typescript-eslint/no-unused-expressions": "warn",
        "react-hooks/exhaustive-deps": "warn"
},
    "overrides": [
    {
        "files": ["**/*.ts", "**/*.tsx"],
        "rules": {
            "@typescript-eslint/no-unused-vars": ["warn", {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }]
        }
    }
]
}